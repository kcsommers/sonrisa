{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": false,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "resolveJsonModule": true,
    "paths": {
      "*": ["node_modules/*", "src/types/*"]
    },
    "skipLibCheck": true,
    "noEmit": false
  },
  "include": ["src/**/*"],
  "exclude": ["instagram_token.json"]
}
